###############################################################################
# CSU configuration file (.csi)
#
# author: Carlos Souza
###############################################################################

###############################################################################
# CSU input parameters
[input]
# Total time [s]
t = 5000
#
# Time step dt [s]
dt = 2
#
# Mesh file name
meshName = case1.mshsim
#
# Number of intermediary saves
Nsave = 4
#
# Output file name
outName = case1.csv
#
###############################################################################
[material = steel] 
# Density [kg/m3]
rho = 7800
#
# Heat capacity [J/K kg]
Cp = 473
#
# Condutivity [Wm/K]
k = 43
#
#------------------------------------------------------------------------------
[material = aluminum] 
# Density [kg/m3]
rho = 2800
#
# Heat capacity [J/K kg]
Cp = 795
#
# Condutivity [Wm/K]
k = 143
#
###############################################################################
# Exemple: [Boundary = marker]
# Type options: constant temperature, heat flux, domain, interface
# Input options: for constant temperature: temperature
#                for heat flux: heat flux, 
#                               convection coefficient, recuperation temperature
#                               emissivity, environmental temperature
#                for interface: None
#                for domain: initial temperature, material
# Units: SI
#------------------------------------------------------------------------------
[boundary = 1]
#
type = constant temperature
#
temperature = 500
#
#------------------------------------------------------------------------------
[boundary = 2]
#
type = constant temperature
#
temperature = 300
#
#------------------------------------------------------------------------------
[boundary = 3]
#
type = heat flux
#
heat flux = 0
#
#------------------------------------------------------------------------------
[boundary = 4]
#
type = heat flux
#
heat flux = 0
#
#------------------------------------------------------------------------------
[boundary = 5]
#
type = domain
#
initial temperature = 400
#
material = steel
#
